{
  "name": "chendachao",
  "homepage": "https://github.com/chendachao/chendachao.github.io",
  "version": "1.6.18",
  "description": "Larry's Home Page",
  "main": "src/index.js",
  "scripts": {
    "setnode": "bash ./scripts/nvm.sh",
    "start": "webpack-dev-server -d --env development",
    "build": "webpack --env production",
    "build:w": "webpack --env production --watch --progress --devtool eval-cheap-source-map",
    "analyse": "webpack --env production --profile --json > stats.json",
    "release": "standard-version",
    "version": "npm run release --commit-all",
    "version:patch": "npm run release -- --release-as patch",
    "version:minor": "npm run release -- --release-as minor",
    "version:major": "npm run release -- --release-as major",
    "push": "git push --follow-tags",
    "--deploy": "npm run version && npm run push",
    "deploy:patch": "npm run version:patch && npm run push",
    "deploy:minor": "npm run version:minor && npm run push",
    "deploy:major": "npm run version:major && npm run push",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist -b gh-pages -u 'chendachao <chendachao@outlook.com>' -r https://chendachao:$GITHUB_TOKEN@github.com/chendachao/chendachao.github.io.git",
    "deploy:local": "npm run version:patch && cross-env GITHUB_TOKEN=$GITHUB_TOKEN node scripts/publish.js"
  },
  "husky": {
    "hooks": {
      "--pre-push": "yarn ps",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "autoprefixer": "9.8.6",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "6.0.5",
    "copy-webpack-plugin": "6.1.0",
    "cross-env": "7.0.2",
    "css-loader": "5.0.0",
    "dotenv": "8.2.0",
    "error-overlay-webpack-plugin": "0.4.1",
    "file-loader": "6.1.1",
    "gh-pages": "3.1.0",
    "glob-all": "3.2.1",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "husky": "^4.3.0",
    "image-webpack-loader": "7.0.1",
    "mini-css-extract-plugin": "1.2.1",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss-loader": "3.0.0",
    "purgecss-webpack-plugin": "3.0.0",
    "script-ext-html-webpack-plugin": "2.1.4",
    "standard-version": "9.0.0",
    "style-loader": "2.0.0",
    "stylelint": "13.7.2",
    "terser-webpack-plugin": "4.2.3",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.3.0",
    "webpack-notifier": "1.8.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "axios": "0.20.0",
    "bowser": "2.11.0",
    "dialog-polyfill": "0.5.3",
    "intl-messageformat": "9.3.16",
    "intro.js": "3.0.1",
    "normalize.css": "8.0.1",
    "qrcode": "1.4.4",
    "sw-offline-google-analytics": "0.0.25",
    "tippy.js": "6.2.7",
    "toastedjs": "0.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chendachao/chendachao.github.io.git"
  },
  "keywords": [
    "Homepage",
    "Larry Chen",
    "Webpack"
  ],
  "author": "chendachao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chendachao/chendachao.github.io/issues"
  }
}
