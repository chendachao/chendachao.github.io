{
  "name": "chendachao",
  "homepage": "https://github.com/chendachao/chendachao.github.io",
  "version": "1.10.10",
  "description": "Larry's Home Page",
  "main": "src/index.js",
  "scripts": {
    "setnode": "bash ./scripts/nvm.sh",
    "trace:start": "node --trace-deprecation node_modules/webpack/bin/webpack.js --mode development --env development",
    "start": "webpack-cli serve --env development",
    "build": "cross-env DOTENV_CONFIG_PATH=./.env.prod webpack build --mode production --env production",
    "build:w": "cross-env DOTENV_CONFIG_PATH=./.env.prod webpack build --mode production --env production --watch --progress --devtool eval-cheap-source-map",
    "serve": "http-server dist -c-1",
    "analyse": "cross-env DOTENV_CONFIG_PATH=./.env.prod webpack build --mode production --env production --profile --json > stats.json",
    "analyse:forward": "cross-env DOTENV_CONFIG_PATH=./.env.prod webpack build --mode production --env production smp",
    "analyse:backward": "cross-env DOTENV_CONFIG_PATH=./.env.prod webpack build --mode production --env production analyse",
    "prelint": "mkdir -p ./.tmp",
    "lint:styles": "yarn prelint && stylelint ./src/**/*.{css,scss} > ./.tmp/styles.txt",
    "lint:scripts": "yarn prelint && eslint ./src > ./.tmp/scripts.txt",
    "lint:prettier": "prettier --list-different '{,!(node_modules)/**/}*.html'",
    "format:styles": "stylelint ./src/**/*.{css,scss} --fix",
    "format:scripts": "eslint ./src --fix",
    "format:prettier": "prettier --write '{,!(node_modules)/**/}*.html'",
    "release": "standard-version",
    "version": "npm run release --commit-all",
    "version:patch": "npm run release -- --release-as patch",
    "version:minor": "npm run release -- --release-as minor",
    "version:major": "npm run release -- --release-as major",
    "push": "git push --follow-tags origin main",
    "--deploy": "npm run version && npm run push",
    "deploy:patch": "npm run version:patch && npm run push",
    "deploy:minor": "npm run version:minor && npm run push",
    "deploy:major": "npm run version:major && npm run push",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist -b gh-pages -u 'chendachao <chendachao@outlook.com>' -r https://chendachao:$GITHUB_TOKEN@github.com/chendachao/chendachao.github.io.git",
    "deploy:local": "npm run version:patch && cross-env GITHUB_TOKEN=$GITHUB_TOKEN node scripts/publish.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ],
    "*.html": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/eslint-parser": "7.13.14",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.13.15",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "autoprefixer": "9.8.6",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "7.1.2",
    "copy-webpack-plugin": "8.1.1",
    "cross-env": "7.0.3",
    "css-loader": "5.2.4",
    "css-minimizer-webpack-plugin": "2.0.0",
    "dotenv": "8.2.0",
    "error-overlay-webpack-plugin": "0.4.2",
    "esbuild-loader": "2.12.0",
    "eslint": "7.26.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-standard": "5.0.0",
    "gh-pages": "3.1.0",
    "glob-all": "3.2.1",
    "html-webpack-plugin": "5.3.1",
    "http-server": "0.12.3",
    "husky": "6.0.0",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "1.5.0",
    "postcss-loader": "3.0.0",
    "prettier": "2.3.0",
    "process": "0.11.10",
    "purgecss-webpack-plugin": "4.0.3",
    "react-dev-utils": "11.0.4",
    "script-ext-html-webpack-plugin": "2.1.5",
    "speed-measure-webpack-plugin": "1.5.0",
    "standard-version": "9.2.0",
    "style-loader": "2.0.0",
    "stylelint": "13.13.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.19.0",
    "terser-webpack-plugin": "5.1.1",
    "tslib": "2.2.0",
    "webpack": "5.35.1",
    "webpack-bundle-analyzer": "4.4.1",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3",
    "webpack-notifier": "1.13.0",
    "webpackbar": "5.0.0-3",
    "workbox-webpack-plugin": "6.1.5"
  },
  "dependencies": {
    "@sentry/browser": "6.3.6",
    "@sentry/tracing": "6.3.6",
    "axios": "0.21.1",
    "bowser": "2.11.0",
    "core-js": "3.12.1",
    "dialog-polyfill": "0.5.6",
    "intl-messageformat": "9.6.11",
    "intro.js": "3.0.1",
    "lazysizes": "5.3.2",
    "normalize.css": "8.0.1",
    "qrcode": "1.4.4",
    "regenerator-runtime": "0.13.7",
    "svgxuse": "1.2.6",
    "sw-offline-google-analytics": "0.0.25",
    "tippy.js": "6.3.1",
    "toastifier": "1.0.8",
    "toastr": "2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chendachao/chendachao.github.io.git"
  },
  "keywords": [
    "Homepage",
    "Larry Chen",
    "Webpack"
  ],
  "author": "chendachao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chendachao/chendachao.github.io/issues"
  }
}
